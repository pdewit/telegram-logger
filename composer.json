{
  "name": "pdewit/telegram-logger",
  "description": "Quick way to log events to Telegram",
  "type": "library",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Patrick de Wit",
      "email": "patrick1193@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Pdewit\\TelegramLogger\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Pdewit\\TelegramLogger\\Tests\\": "tests"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "require": {
    "illuminate/support": "^5.3",
    "unreal4u/telegram-api": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "orchestra/testbench": "~3.0"
  }
}
